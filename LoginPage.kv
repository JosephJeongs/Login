<LoginManager>:
    LoginPageScreen:
        name: "loginpage"
    AccountRegisterScreen:
        name: "registerpage"
    LoggedInScreen:
        name: "loggedinpage"

<LoginPageScreen>:
    orientation: "vertical"
    Button:
        text: "Login"
        size_hint: 1,.3

    BoxLayout:
        orientation: "horizontal"
        spacing: "25dp"
        Button:
            text: "Username:"
            size_hint: .15,1
        Button
            text: "Password:"
            size_hint: .15,.01



